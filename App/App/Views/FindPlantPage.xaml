<?xml version="1.0" encoding="utf-8" ?>
<ContentPage Title="Find Plants" xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App.Views.FindPlantPage">
    <ContentPage.Content>
        <StackLayout Padding="5">
           
            <Label Text="ID"/>
            <Entry Text="{Binding ID}" />

            <Button Text="Search"/>
            <ListView x:Name="PlantData" BackgroundColor="LightGreen" >

            </ListView>
        </StackLayout>
        
    </ContentPage.Content>
</ContentPage>