{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "max-len": ["error", { "code": 100 }],
        "linebreak-style": ["error", "windows"]
    }
}
